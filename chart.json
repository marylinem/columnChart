{
	"id": "com.demo.chart",
	"version": "1.0.0",
	"name": "Demo chart",
	"description": "A chart demo",
	"newInstancePrefix": "chart",
	"icon": "https://marylinem.github.io/columnChart/chart/icon.png",
	"vendor": "Demo",
	"eula": "EULA",
	"license": "1.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-demo-chart",
			"url": "https://marylinem.github.io/columnChart/chart/chart.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"value": {
			"type": "number",
			"description": "chart value",
			"default": 100
		},
		"width": {
			"type": "integer",
			"default": 1000
		},
		"height": {
			"type": "integer",
			"default": 1000
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the chart value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "chart value"
				},
				{
					"name": "newInfo",
					"type": "string",
					"description": "chart info"
				},
				{
					"name": "newColor",
					"type": "string",
					"description": "Text Color info"
				}
			],
			"body": "this.value = newValue; this.info = newInfo; this.color = newColor;"
		},
		"getValue": {
			"returnType": "number",
			"description": "Returns the chart value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Colored Box."
		}		
	}
}

 